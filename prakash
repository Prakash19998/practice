pipeline {
    agent any 
         stages {
            stage ('Testing') {
               steps {
                echo 'Hello Prakash'
                }
             }
             stage ('Confirmation') {
               steps {
                input ('Do you wanna continue?')
                }
             }
             stage ('Done') {
               steps {
                echo 'Bye Prakash'
                }
             }
          }
      }
