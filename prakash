pipeline {
    agent any 
         stages {
            stage ('Testing') {
               steps {
                echo 'Hello Prakash'
                }
             }
             stage ('Confirmation') {
               steps {
              echo 'Do you wanna continue with me'
                }
             }
             stage ('Done') {
               steps {
                echo 'Bye Prakash'
                }
             }
stage ('Done1') {
               steps {
                echo 'Bye Prakash'
                }
             }
stage ('Done2') {
               steps {
                echo 'Bye Prakash'
                }
             }
          }
      }
